[run]
source = abydos
omit =
    */python?.?/*
    */lib-python/?.?/*.py
    */lib_pypy/_*.py
    */site-packages/ordereddict.py
    */site-packages/nose/*
    */unittest2/*
    */tests/*
    */__init__.py
parallel = True
branch = True

[report]
exclude_lines =
    if __name__ == .__main__.:
    if PY3:
omit =
    abydos/__init__.py
